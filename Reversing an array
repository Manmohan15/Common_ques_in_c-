#include<bits/stdc++.h>

using namespace std;

int main()
{
    int arr[]={1,2,3,4,5};
	int n=sizeof(arr)/sizeof(arr[0]);
	for(int i=0;i<ceil(n/2);i++)
	{
		int t=arr[n-1-i];
		arr[n-1-i]=arr[i];
		arr[i]=t;
		
	}
//    int start=0;
//    int end=sizeof(arr);
//    while(start<end)
//    {
//    	int t=arr[start];
//    	arr[start]=arr[end];
//    	arr[end]=arr[start];
//    	start+=1;
//    	end-=1;
//	}
    cout<<"size"<<sizeof(arr)/sizeof(arr[0])<<endl;
	for(int j=0;j<sizeof(arr)/sizeof(arr[0]);j++)
	{
		
		cout<<arr[j]<<endl;
	}
	return 0;
}
