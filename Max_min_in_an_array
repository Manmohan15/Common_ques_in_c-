#include<iostream.h>

using namespace std;

int main()
{

	int arr[]={1,2,3,4,5};
	int min=INT_MAX;
	int max=INT_MIN;
	for(int i=0;i<sizeof(arr)/sizeof(arr[0]);i++)
	{
		if(arr[i]>max)
		{
			max=arr[i];
		}
		elif(arr[i]<min)
		{
			min=arr[i];
		}
	}
	cout<<min<<endl;
	cout<<max<<endl;

//	for(int j=0;j<sizeof(arr)/sizeof(arr[0]);j++)
//	{
//		
//		cout<<arr[j]<<endl;
//	}
	return 0;
}
